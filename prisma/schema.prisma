// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Employee {
  empId    Int @unique
  name     String
  address  String
  classification String
}

model HourlyClassification {
  empId    Int @unique
  rate     Float
}

model TimeCard {
  empId     Int
  date      DateTime
  hours     Int

  @@unique([empId, date])
}
